[DEFAULT]
arch = %(architecture)s
ip = dhcp
part = %(working_dir)s/%(suite)s.partition
user = mig
name = MiG User
pass = mig
tmpfs = - 
firstboot = %(working_dir)s/boot-%(suite)s.sh
mem = %(memory)d
cpus = %(cpu_count)d

[ubuntu]
mirror = http://127.0.0.1:9999/ubuntu
suite = precise
flavour = virtual
addpkg = %(package_list)s
